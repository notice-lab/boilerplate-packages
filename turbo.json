{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "clean": {
      "dependsOn": []
    },
    "format:typescript": {
      "cache": true,
      "dependsOn": []
    },
    "format:eslint": {
      "cache": true,
      "dependsOn": []
    },
    "format:stylelint": {
      "cache": true,
      "dependsOn": []
    },
    "format:prettier": {
      "cache": true,
      "dependsOn": []
    },
    "format:typedoc": {
      "cache": true,
      "dependsOn": []
    },
    "format:update-license": {
      "cache": true,
      "dependsOn": []
    },
    "test:coverage": {
      "cache": true,
      "dependsOn": [],
      "outputs": ["coverage/**"]
    }
  }
}
