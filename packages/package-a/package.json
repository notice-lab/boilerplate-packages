{
  "name": "@sprovod-config/package-a",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://sprovod-336911096458.d.codeartifact.eu-central-1.amazonaws.com/npm/sprovod-config/"
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "build:dev": "tsup --watch",
    "bump": " npm version patch",
    "clean": "rm -rf build coverage .turbo",
    "format": "npm-run-all format:*",
    "format:typescript": "tsc",
    "format:eslint": "eslint --cache './src/**/*'",
    "format:prettier": "prettier --cache --write './src/**/*.{ts,tsx}' --log-level silent",
    "format:update-license": "eslint --cache --fix --no-eslintrc --config ./scripts/eslint.header.js './src/**/*'",
    "test": "jest --cache --runInBand --passWithNoTests",
    "test:dev": "jest --cache --watch --runInBand --passWithNoTests",
    "test:coverage": "jest --cache --coverage --passWithNoTests"
  },
  "devDependencies": {
    "@sprovod-config/eslint-config": "latest",
    "@sprovod-config/jest-config": "latest",
    "@sprovod-config/prettier-config": "latest",
    "@sprovod-config/typescript-config": "latest",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
